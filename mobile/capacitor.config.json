{
    "appId": "com.ofta.game",
    "appName": "One for the Ages",
    "webDir": "out",
    "bundledWebRuntime": false,
    "plugins": {
        "SplashScreen": {
            "launchShowDuration": 2000,
            "backgroundColor": "#1a1a2e",
            "showSpinner": false,
            "splashFullScreen": true,
            "splashImmersive": true
        },
        "StatusBar": {
            "style": "light",
            "backgroundColor": "#1a1a2e",
            "overlaysWebView": false
        },
        "Haptics": {},
        "Share": {},
        "PushNotifications": {
            "presentationOptions": [
                "badge",
                "sound",
                "alert"
            ]
        }
    }
}